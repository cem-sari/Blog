<%- include ("../partials/header") %>

<header class="masthead" style="background-image: url('https://images.unsplash.com/photo-1623851467157-5eb7199d0f18?ixid=MnwxMjA3fDB8MHxlZGl0b3JpYWwtZmVlZHwzfHx8ZW58MHx8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60')">
    <div class="container position-relative px-4 px-lg-5">
        <div class="row gx-4 gx-lg-5 justify-content-center">
            <div class="col-md-10 col-lg-8 col-xl-7">
                <div class="page-heading">
                    <h1>Admin Dash</h1>
                    <span class="subheading">This is what I do.</span>
                </div>
            </div>
        </div>
    </div>
</header>

<h1>Admin Page</h1>



<div class="container">
    <div class="d-flex justify-content-center">
        <% adminActions.forEach( (action)=>{ %>
        <div class="p-2">
            <button id="<%= action.actionId %>" onclick="catchId(this.id)" type="button" class="btn btn-outline-primary btn-block"> <%= action.displayName %> </button>
        </div>
        <% }); %> 
    </div>
</div>

<script>

function catchId(clicked_id){
    alert(clicked_id + " Numarali butona tiklandi.");
};

</script>

